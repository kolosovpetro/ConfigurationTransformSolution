{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DatabaseUrl": "#{DatabaseUrlValue}#",
    "DataSource": "#{DataSourceValue}#",
    "InitialCatalog": "#{InitialCatalogValue}#",
    "UserId": "#{UserIdValue}#",
    "Password": "#{PasswordValue}#"
  }
}
